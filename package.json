    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "npm": "^9.8.1",
    "or": "^0.2.0",
    "path": "^0.12.7",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.5.1",
    "concurrently": "^8.2.0",
    "mongoose": "^7.4.3",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  }
