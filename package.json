{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/app.js\"",
    "migrate:up": "migrate-mongoose up",
    "migrate:down": "migrate-mongoose down",
    "create:migration": "migrate-mongoose create",
    "rollback:migration": "migrate-mongoose rollback",
    "migrate:Api": "ts-node ./src/db/migrate.api.ts",
    "migrate:Roles": "ts-node ./src/db/migrate.roles.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
